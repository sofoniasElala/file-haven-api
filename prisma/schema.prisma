generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Session {
  id        String   @id
  sid       String   @unique
  data      String    @db.Text
  expiresAt   DateTime
}

model user {
    id Int @id @default(autoincrement())
    username String @db.VarChar(25) @unique
    password String @db.VarChar(64)

}
